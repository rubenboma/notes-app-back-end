{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "indent":"off",
        "no-unused-vars":"off",
        "no-trailing-spaces":"off",
        "space-before-blocks":"off",
        "consistent-return":"off",
        "object-curly-newline":"off"

    }
}
